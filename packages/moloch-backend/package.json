{
  "name": "moloch-backend",
  "version": "1.0.0",
  "description": "Backend for the Moloch app.",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc es2017 --watch --outDir dist",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "migrate": "node ./dist/src/migrate",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test",
    "dev": "concurrently --kill-others \"npm run build:watch\" \"nodemon .\""
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^1.0.10",
    "@loopback/context": "^1.4.1",
    "@loopback/core": "^1.1.4",
    "@loopback/openapi-v3": "^1.1.7",
    "@loopback/repository": "^1.1.3",
    "@loopback/rest": "^1.5.3",
    "@loopback/rest-explorer": "^1.1.6",
    "@loopback/service-proxy": "^1.0.6",
    "dotenv": "^6.2.0",
    "memory": "0.0.3",
    "node-cron": "^2.0.3",
    "web3": "^1.0.0-beta.46"
  },
  "devDependencies": {
    "@loopback/build": "^1.2.0",
    "@loopback/testlab": "^1.0.4",
    "@loopback/tslint-config": "^2.0.0",
    "@types/node": "^10.11.2",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10",
    "tslint": "^5.12.0",
    "typechain": "^0.3.11",
    "typescript": "^3.2.2"
  }
}
